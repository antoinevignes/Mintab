{
  "manifest_version": 2,
  "name": "Custom New Tab",
  "version": "1.0",
  "description": "A custom new tab page with welcome message, date and time.",
  "chrome_settings_overrides": {
    "homepage": "index.html"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["storage"]
}
